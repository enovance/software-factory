[ircbot]
nick=<%= @gerritbot['botname'] %>
pass=<%= @gerritbot['password'] %>
server=<%= @gerritbot['ircserver'] %>
port=<%= @gerritbot['ircport'] %>
channel_config=/etc/gerritbot_channels.yaml
pid=/var/run/gerritbot.pid
log_config=/etc/gerritbot_logging.config

[gerrit]
user=jenkins
key=/var/lib/jenkins/.ssh/id_rsa
host=gerrit
port=29418
