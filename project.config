[project]
	description = The software-factory Project
[access "refs/heads/*"]
	label-Approved = +0..+1 group SoftwareFactory-core
	label-Code-Review = -2..+2 group SoftwareFactory-core
	label-Verified = -2..+2 group Non-Interactive Users
	submit = group SoftwareFactory-core
	read = group Registered Users
	label-Workflow = -1..+1 group Registered Users
[access "refs/*"]
	owner = group SoftwareFactory-ptl
	read = group SoftwareFactory-core
	pushTag = +force group SoftwareFactory-ptl
[access "refs/meta/config"]
	read = group Registered Users
[submit]
	action = rebase if necessary
[access "refs/tags/*"]
	push = +force group SoftwareFactory-ptl
